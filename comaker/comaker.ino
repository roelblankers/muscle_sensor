  if(wissel == 1){
    if (sensorValue > refValue){
      if (state == 1){
        digitalWrite (13,1);
        digitalWrite(transmitPin, 1);
        state = 0;
      }
    }
  }
}

void setup() {
  pinMode(13,OUTPUT);
  pinMode(audioPin, OUTPUT);
  pinMode(refPin, INPUT);
  pinMode(transmitPin, OUTPUT);
  lcd.begin(16, 2);
  lcd.init();
  lcd.backlight();
  lcd.home();
}

void dingen(){ //booleaanse staat weer op 1 zetten na het ontspannen van de spieren
