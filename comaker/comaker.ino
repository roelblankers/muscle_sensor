  Serial.print("\tReferentie = ");
  Serial.println(refValue);
  }

void sound(){ //muziek afspelen na threshold waarde
  if (wissel == 0){
    if (sensorValue > refValue){
      if (state == 1){
        digitalWrite(audioPin, HIGH);
        startPlayback(sample, sizeof(sample));
        state = 0;
      }
    }
  }
}

